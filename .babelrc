{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "entry"
    }],
    "react-app"
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "Api": "./src/api",
        "Clients": "./src/serviceClients",
        "Components": "./src/components",
        "Containers": "./src/containers",
        "Lib": "./src/lib",
        "Models": "./src/models",
        "Services": "./src/applicationService",
        "Src": "./src",
        "Storybook": "./.storybook",
        "Types": "./src/types",
        "Utils": "./src/utils"
      }
    }]
  ]
}
