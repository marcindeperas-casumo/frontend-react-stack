{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "3.6.4"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "babel-plugin-macros",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "Api": "./src/api",
          "Clients": "./src/serviceClients",
          "Components": "./src/components",
          "Pages": "./src/pages",
          "Layouts": "./src/layouts",
          "Containers": "./src/containers",
          "Features": "./src/features",
          "Lib": "./src/lib",
          "Models": "./src/models",
          "Services": "./src/applicationService",
          "Src": "./src",
          "Storybook": "./.storybook",
          "Types": "./src/types",
          "Utils": "./src/utils",
          "GameProviders": "./src/gameProviders"
        }
      }
    ]
  ]
}
