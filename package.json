    "sass-loader": "^7.1.0",
    "sinon": "^7.2.4",
    "storybook-chromatic": "^1.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-order": "^2.0.0",
    "stylelint-prettier": "^1.0.6",
