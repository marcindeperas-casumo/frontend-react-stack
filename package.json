{
  "name": "mobile-react-stack-poc",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@casumo/cmp-badge": "^1.2.2",
    "@casumo/cmp-button": "^2.0.2",
    "@casumo/cmp-card": "^2.0.2",
    "@casumo/cmp-flex": "^2.2.2",
    "@casumo/cmp-icons": "^1.3.0",
    "@casumo/cmp-picture": "^1.0.0",
    "@casumo/cmp-responsive-image": "^1.3.1",
    "@casumo/cmp-scrollable": "^2.2.2",
    "@casumo/cmp-skeleton": "^1.2.1",
    "@casumo/cmp-text": "1.2.2",
    "@casumo/cudl": "5.1.1",
    "@researchgate/react-intersection-observer": "^0.7.3",
    "classnames": "^2.2.6",
    "cometd": "^3.1.4",
    "intersection-observer": "^0.5.0",
    "invariant": "^2.2.4",
    "luxon": "^1.4.3",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "qs": "^6.5.2",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-icon-base": "2.1.2",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.1.39",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "sentence-case": "^2.1.1"
  },
  "scripts": {
    "flow": "flow",
    "precommit": "scripts/pre-commit.sh && scripts/forgotten-statements.sh",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:ci": "CI=yes npm run test",
    "format": "eslint --fix './src/**/*.js' && stylelint --fix './src/**/*.scss'",
    "storybook": "start-storybook -p 6006",
    "chromatic": "CI=true CHROMATIC_APP_CODE=$REACT_POC_CHROMATIC_APP_CODE chromatic test --script-name storybook --exit-zero-on-changes",
    "generate-component": "./scripts/generate-component.js ./src/components"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/enzyme.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --parser markdown --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "flow"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "babel-plugin-webpack-alias",
            {
              "config": "./config/webpack.config.dev.js"
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.10",
    "@storybook/addon-info": "4.0.0-alpha.10",
    "@storybook/addon-knobs": "4.0.0-alpha.10",
    "@storybook/addon-links": "4.0.0-alpha.10",
    "@storybook/addon-viewport": "^3.4.11",
    "@storybook/addons": "4.0.0-alpha.10",
    "@storybook/react": "4.0.0-alpha.10",
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.5",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.11",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-import-resolver-babel-module-alias": "^1.5.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.74.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "is-promise": "^2.1.0",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "mock-socket": "^8.0.2",
    "node-sass": "^4.9.0",
    "postcss-loader": "2.1.5",
    "prettier": "^1.13.7",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react-svg-loader": "^2.1.0",
    "resolve": "1.6.0",
    "sass-loader": "^7.0.3",
    "storybook-chromatic": "^1.0.2",
    "style-loader": "0.19.0",
    "stylelint": "^9.2.1",
    "stylelint-config-prettier": "^3.3.0",
    "stylelint-config-sass-guidelines": "^4.1.0",
    "stylelint-order": "^0.8.0",
    "stylelint-prettier": "0.2.1",
    "stylelint-scss": "^2.3.0",
    "stylelint-selector-pattern": "^0.0.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  }
}
