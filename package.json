  "scripts": {
    "flow": "flow",
    "start": "node scripts/start.js",
    "dev": "yarn start",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "pact": "jest --testMatch '<rootDir>/pact/*.pact.js'",
    "pact:publish": "node pact/publish.js",
    "pact:can-i-deploy": "node pact/can-i-deploy.js",
    "pact:ci": "yarn pact && yarn pact:publish && yarn pact:can-i-deploy",
    "test:ci": "CI=yes npm run test --coverage",
    "sonar": "node sonar-project.js",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint '**/*.js'",
    "lint:css": "stylelint '**/*.scss'",
    "format": "eslint --fix '**/*.js' && stylelint --fix '**/*.scss'",
    "storybook": "start-storybook -s ./.storybook/static -p 6006",
    "storybook:build": "build-storybook -c .storybook -s ./.storybook/static -o .out",
    "chromatic": "CI=true CHROMATIC_APP_CODE=$REACT_POC_CHROMATIC_APP_CODE chromatic test --build-script-name storybook:build --exit-zero-on-changes",
    "generate-component": "./scripts/generate-component.js ./src/components",
    "graphql-generate": "node ./scripts/graphql-generate.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn flow"
    }
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/enzyme.js",
      "<rootDir>/config/jest/matchMedia.js",
      "<rootDir>/config/jest/mutationObserverMock.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/(index|types).js",
      "!src/**/*.(stories|types).js",
      "!src/**/types/**"
    ],
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "lcov",
      "html",
      "text"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/config/jest/localStorageMock.js"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^.+\\.(gql|graphql)$": "jest-transform-graphql",
      "^(?!.*\\.(js|jsx|mjs|json|gql|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.svg$": "<rootDir>/__mocks__/svgr.js",
      "^.+\\.module\\.(sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "mjs"
    ],
    "restoreMocks": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "*.{md,json,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
