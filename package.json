    "stylelint-order": "2.0.0",
    "stylelint-prettier": "1.0.6",
    "stylelint-scss": "3.5.1",
    "stylelint-selector-pattern": "0.0.2",
    "terser-webpack-plugin": "1.2.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4"
  }
}
