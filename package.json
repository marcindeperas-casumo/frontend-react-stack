    "@casumo/cmp-scrollable": "2.2.2",
    "@casumo/cmp-skeleton": "1.2.1",
    "@casumo/cmp-table": "^1.0.1",
    "@casumo/cmp-text": "1.2.2",
    "@casumo/cudl": "5.1.1",
    "@researchgate/react-intersection-observer": "0.7.4",
    "classnames": "2.2.6",
    "cometd": "3.1.7",
    "intersection-observer": "0.5.1",
    "invariant": "2.2.4",
    "luxon": "1.10.0",
    "normalizr": "3.3.0",
    "qs": "6.6.0",
    "ramda": "0.26.1",
    "react": "16.7.0",
    "react-dev-utils": "7.0.1",
    "react-dom": "16.7.0",
    "react-icon-base": "2.1.2",
    "react-loadable": "5.5.0",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "redux-saga": "0.16.2",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "rollbar": "2.5.1",
    "store": "2.0.12",
    "wait-for-element": "1.0.2"
  },
  "scripts": {
    "flow": "flow check",
    "start": "node scripts/start.js",
    "dev": "npm run start",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:ci": "CI=yes npm run test",
    "lint": "eslint './src/**/*.js' && stylelint './src/**/*.scss'",
    "format": "eslint --fix './src/**/*.js' && stylelint --fix './src/**/*.scss'",
    "storybook": "start-storybook -s ./.storybook/static -p 6006",
    "chromatic": "CI=true CHROMATIC_APP_CODE=$REACT_POC_CHROMATIC_APP_CODE chromatic test --script-name storybook --exit-zero-on-changes",
    "generate-component": "./scripts/generate-component.js ./src/components"
  },
  "husky": {
    "hooks": {
      "pre-commit": "scripts/forgotten-statements.sh && flow"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/enzyme.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.{js,jsx}"
    ],
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "lcov",
      "html",
      "text"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "mjs"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --parser markdown --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@storybook/addon-actions": "4.1.6",
    "@storybook/addon-backgrounds": "4.1.6",
    "@storybook/addon-info": "4.1.6",
    "@storybook/addon-knobs": "4.1.6",
    "@storybook/addon-links": "4.1.6",
    "@storybook/addon-viewport": "4.1.6",
    "@storybook/addons": "4.1.6",
    "@storybook/core": "4.1.6",
    "@storybook/react": "4.1.6",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.1.2",
    "babel-plugin-named-asset-import": "0.3.0",
    "babel-preset-react-app": "7.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.2",
    "css-loader": "2.1.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.12.0",
    "eslint-config-prettier": "3.5.0",
    "eslint-config-react-app": "3.0.6",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-ramda": "2.5.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "flow-bin": "0.90.0",
    "fs-extra": "7.0.1",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "mock-raf": "1.0.1",
    "mock-socket": "8.0.5",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
