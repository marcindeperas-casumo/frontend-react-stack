  "scripts": {
    "flow": "flow",
    "start": "node scripts/start.js",
    "dev": "npm run start",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:ci": "CI=yes npm run test --coverage",
    "lint": "eslint '**/*.js' && stylelint '**/*.scss'",
    "format": "eslint --fix '**/*.js' && stylelint --fix '**/*.scss'",
    "storybook": "start-storybook -s ./.storybook/static -p 6006",
    "storybook:build": "build-storybook -c .storybook -s ./.storybook/static -o .out",
    "chromatic": "CI=true CHROMATIC_APP_CODE=$REACT_POC_CHROMATIC_APP_CODE chromatic test --script-name storybook --exit-zero-on-changes",
    "generate-component": "./scripts/generate-component.js ./src/components",
    "graphql-generate-types": "apollo codegen:generate flow-typed/graphql  --target=flow --endpoint=http://localhost:4000 --includes=\"src/components/**/*{js,graphql}\" --outputFlat --no-addTypename",
    "graphql-generate-types-sports": "apollo codegen:generate flow-typed/graphql  --target=flow --endpoint=http://localhost:4000 --includes=\"src/features/sports/**/*{js,graphql}\" --outputFlat --no-addTypename"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn flow"
    }
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/enzyme.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.{js,jsx}"
    ],
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "lcov",
      "html",
      "text"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/config/jest/localStorageMock.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "mjs"
    ],
    "restoreMocks": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "*.{md,json,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-backgrounds": "^5.0.3",
    "@storybook/addon-info": "^5.0.3",
