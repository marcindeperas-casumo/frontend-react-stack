    "@storybook/core": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "@svgr/webpack": "^4.1.0",
    "@types/jest": "^24.0.0",
    "apollo": "^2.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "base64-inline-loader": "^1.1.1",
