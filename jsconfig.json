{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "Containers/*": ["./src/containers/*"],
      "Components/*": ["./src/components/*"],
      "Features/*": ["./src/features/*"],
      "Models/*": ["./src/models/*"],
      "Services/*": ["./src/applicationService/*"],
      "Clients/*": ["./src/serviceClients/*"],
      "Utils/*": ["./src/utils/*"],
      "Lib/*": ["./src/lib/*"],
      "Src/*": ["./src/*"],
      "Storybook/*": ["./.storybook/*"],
      "Types/*": ["./src/types/*"],
      "Api/*": ["./src/api/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
